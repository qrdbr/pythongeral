x = list(range(5))
print(x) #[0, 1, 2, 3, 4]

y = [5,6,7]

x.extend(y)
print(x) #[0, 1, 2, 3, 4, 5, 6, 7]

### para nao mudar x, utilizar adicao
x1 = list(range(9))
print(x1) #[0, 1, 2, 3, 4, 5, 6, 7, 8]

z = x1 + y
print(z) #[0, 1, 2, 3, 4, 5, 6, 7, 8, 5, 6, 7]

print(x1) #[0, 1, 2, 3, 4, 5, 6, 7, 8]

## apend
z.append(11)

print(z) #[0, 1, 2, 3, 4, 5, 6, 7, 8, 5, 6, 7, 11]
