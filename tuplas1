minha_lista = [1,2]
print(minha_lista)
print(type(minha_lista))
minha_tupla = (1,2)
print(minha_tupla)
print(type(minha_tupla))
outra_tupla = 1,2
print(outra_tupla)
print(type(outra_tupla))

minha_lista[1] = 3
print(minha_lista)

try:
    minha_tupla[1] = 33
except TypeError as err:
    print("Operacao nao permitida:", err)
    
''''
RESULTADO
[1, 2]
<class 'list'>
(1, 2)
<class 'tuple'>
(1, 2)
<class 'tuple'>
[1, 3]
Operacao nao permitida: 'tuple' object does not support item assignment

'''
